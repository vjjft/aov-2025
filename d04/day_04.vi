

pub mod floor = "./floor.vi";
use floor::Floor;

pub fn main(&io: &IO) {
  let part1 = 0[N64];
  let part2 = 0[N64];

  let floor = Floor::parse_input(&io);

  floor.notify()
  part1 = floor.count_can_lift()

  let total = 0[N64];
  loop {
    floor.notify();
    let lifted = floor.lift();
    total += lifted;
    if lifted > 0[N64] {
      continue;
    };
  }

  part2 = total;
  io.println("Part 1: {part1}");
  io.println("Part 2: {part2}");
}
